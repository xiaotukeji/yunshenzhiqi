location / {
    if (!-e $request_filename) {
    rewrite ^(.*)$ /index.php/$1 last;
    break;
   }
}

location /h5{
     if (!-e $request_filename) {
     rewrite ^/(.*) /h5/index.html last;
     break;
     }
    }

location /web{
     if (!-e $request_filename) {
     rewrite ^/(.*) /web/index.html last;
     break;
     }
    }

location /mshop {
 if (!-e $request_filename) {
  rewrite  ^(.*)$  /mshop/index.html  last; 
  break;
 }
}
